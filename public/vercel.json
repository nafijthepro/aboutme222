{
  "rewrites": [
    {
      "source": "/nafij",
      "destination": "/"
    },
    {
      "source": "/nafijpro",
      "destination": "/"
    },
    {
      "source": "/nafij-the-pro",
      "destination": "/"
    },
    {
      "source": "/nafijur-rahaman",
      "destination": "/"
    },
    {
      "source": "/nafij-rahaman",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}